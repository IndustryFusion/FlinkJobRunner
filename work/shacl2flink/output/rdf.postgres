  GRANT USAGE, SELECT ON SEQUENCES TO flink_repl_user;


DROP TABLE IF EXISTS "rdf";
CREATE TABLE "rdf" (
"subject" TEXT,
"predicate" TEXT,
"object" TEXT,
"index" BIGINT,
PRIMARY KEY("subject","predicate","index")
);

