@prefix : <https://example.org/ontology#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:CutterShape a sh:NodeShape ;
    sh:targetClass :Cutter ;
    sh:property [
        sh:path :hasWorkpiece ;
        sh:class :Workpiece ;
        sh:maxCount 1 ;
        sh:minCount 0
    ] .

:WorkpieceShape a sh:NodeShape ;
    sh:targetClass :Workpiece .
