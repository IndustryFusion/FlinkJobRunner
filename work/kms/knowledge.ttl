@prefix : <https://example.org/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

: a owl:Ontology .

:Machine a owl:Class .

:Cutter a owl:Class ;
    rdfs:subClassOf :Machine .

:Workpiece a owl:Class .

:hasWorkpiece a owl:ObjectProperty ;
    rdfs:domain :Cutter ;
    rdfs:range :Workpiece .

:state_ON a :MachineState ;
    rdfs:label "ON" .

:state_OFF a :MachineState ;
    rdfs:label "OFF" .

:MachineState a owl:Class .
